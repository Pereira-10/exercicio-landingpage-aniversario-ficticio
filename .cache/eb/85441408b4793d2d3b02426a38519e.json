ncipal: #fafafa;\r\n$corTextoSecundario: #030303;\r\n$corFundo: #fefefe;\r\n$corBotao: #4f2ac3;\r\n\r\n$blue1: #1b6ca7;\r\n$blue2: #442bdb;\r\n$pink: #89223b;\r\n$yellow: #9c791d;\r\n","styles\\components\\_infos_bar.scss":"@use '../variables';\r\n\r\n.infos-bar {\r\n    padding: 16px 24px;\r\n    background-color: variables.$corTextoPrincipal;\r\n    display: flex;\r\n    justify-content: space-between;\r\n\r\n    &__infos {\r\n        display: flex;\r\n        gap: 40px;\r\n\r\n        &__item{\r\n            font-size: 14px;\r\n            color: variables.$corTextoSecundario;\r\n            b {\r\n                font-size: 18px;\r\n                display: block;\r\n                color: variables.$corTextoSecundario;\r\n            }\r\n        }\r\n    }\r\n\r\n    @media (max-width: 640px) {\r\n        display: block;\r\n\r\n        &__infos {\r\n            display: block;\r\n\r\n            &__item {\r\n                margin-bottom: 24px;\r\n            }\r\n        }\r\n    }\r\n\r\n    @media (max-width: 1024px) {\r\n        &__infos {\r\n            gap: 16px;\r\n        }\r\n    }\r\n}\r\n","styles\\components\\_buttons.scss":"@use '../variables';\r\n\r\n.button {\r\n    padding: 12px;\r\n    font-size: 18px;\r\n    font-weight: bold;\r\n    text-decoration: none;\r\n    text-align: center;\r\n    white-space: nowrap;  // sem quebra de linha\r\n\r\n    &--primary {\r\n        color: variables.$corTextoPrincipal;\r\n        background-color: variables.$corBotao;\r\n    }\r\n\r\n    @media (max-width:640px){\r\n        display: block;\r\n        width: 100%;\r\n    }\r\n\r\n    @media (max-width: 1024px) {\r\n        display: flex;\r\n        align-items: center;\r\n    }\r\n}\r\n","styles\\_event.scss":"@use './variables';\r\n\r\n\r\n.event {\r\n    padding: 80px 0;\r\n\r\n    &--frontend {\r\n        background-image: linear-gradient(variables.$blue1, variables.$blue2);\r\n    }\r\n\r\n    &--ux-ui {\r\n        background-image: linear-gradient(variables.$blue2, variables.$pink);\r\n    }\r\n\r\n    &--data-science {\r\n        background-image: linear-gradient(variables.$pink, variables.$yellow);\r\n    }\r\n\r\n    &--backend {\r\n        background-image: linear-gradient(variables.$yellow, variables.$pink);\r\n    }\r\n\r\n    &--image-left {\r\n        .container {\r\n            flex-direction: row-reverse;\r\n        }\r\n    }\r\n    .container {\r\n        display: flex;\r\n        gap: 40px;\r\n    }\r\n\r\n    &__image {\r\n        max-width: 360px;\r\n    }\r\n\r\n    &__details {\r\n    \r\n\r\n        &__title {\r\n            font-size: 18px;\r\n            font-weight: bold;\r\n            margin-bottom: 16px;\r\n        }\r\n\r\n        &__description {\r\n            font-size: 16px;\r\n            line-height: 22px;\r\n        }\r\n    }\r\n\r\n    @media (max-width: 640px) {\r\n        .container {\r\n            display: block;\r\n        }\r\n\r\n        &__image {\r\n            width: 100%;\r\n        }\r\n\r\n        &__details {\r\n            margin-right: 0;\r\n            margin-bottom: 16px;\r\n        }\r\n    }\r\n}\r\n","styles\\main.scss":"\r\n@use './variables';\r\n@use 'hero';\r\n@use 'components/infos_bar';\r\n@use 'components/buttons';\r\n@use 'event';\r\n\r\n* {\r\n    margin: 0;\r\n    padding: 0;\r\n    box-sizing: border-box;\r\n    font-family: 'Roboto', sans-serif;\r\n    color: variables.$corTextoPrincipal;\r\n    list-style: none;\r\n}\r\n\r\n.container {\r\n    max-width: 960px;\r\n    width: 100%;\r\n    margin: 0 auto;\r\n\r\n    @media (max-width: 640px) {\r\n        max-width: 80%;\r\n    }\r\n\r\n    @media (max-width: 1024px){\r\n        max-width: 90%;\r\n    }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"bc8024d958b175e22db44546ff669772","cacheData":{"env":{}}}